{
  "name": "darcorm/yii2-fullcalendar",
  "description": "Yii2 widget for fullcalendar",
  "require": {
    "php": ">=7.4.0",
    "yiisoft/yii2": ">=2.0.40",
    "kriss/yii2-calendar-schedule": "*",
    "npm-asset/fullcalendar": "v6.1.9"
  },
  "keywords": [
    "fullcalendar",
    "javascript",
    "event",
    "calendar"
  ],
  "homepage": "https://github.com/darcorm/yii2-fullcalendar",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Darrell Cormier",
      "email": "darcorm@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "darcorm\\fullcalendar\\": "src"
    }
  },
  "config": {
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  }
}
